{
    "name": "iansltx/gcm-client",
    "description": "A client for Firebase Cloud Messaging for Android's message API (via Legacy endpoints)",
    "keywords": [
        "library", "gcm", "google", "push", "android", "firebase", "fcm"
    ],
    "homepage": "https://github.com/iansltx/gcm-client",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Ian Littman",
            "email": "iansltx@gmail.com",
            "homepage": "http://yanntx.info",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.2",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1",
        "scrutinizer/ocular": "^1.5.2"
    },
    "autoload": {
        "psr-4": {
            "iansltx\\GCMClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iansltx\\GCMClient\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
